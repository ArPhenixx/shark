tshark -r votre_fichier.pcap -Y 'ip.dst == x.x.x.x && (http.content_type == "application/x-msdownload" || http.content_type == "application/x-msdos-program" || http.content_type == "application/octet-stream")' -T fields -e frame.number -e ip.src -e ip.dst -e http.content_type > executables_to_ip.txt
